{
  "schemaVersion": 1,
  "id": "station-resource-loader-v0",
  "version": "${version}",

  "name": "Station Resource Loader (v0)",
  "description": "Handles resource loading, such as textures, tags, etc.",
  "authors": [
    "Modification Station",
    "FloofyPlasma"
  ],
  "contact": {
    "homepage": "https://github.com/FloofyPlasma/StationAPI",
    "sources": "https://github.com/FloofyPlasma/StationAPI",
    "issues": "https://github.com/FloofyPlasma/StationAPI/issues"
  },

  "license": "MIT",
  "icon": "assets/station-resource-loader-v0/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "com.floofyplasma.stationapi.impl.resource.DataReloaderImpl"
    ],
    "stationapi:event_bus_client": [
      "com.floofyplasma.stationapi.impl.client.resource.AssetsReloaderImpl"
    ]
  },
  "mixins": [
    "station-resource-loader-v0.mixins.json"
  ],

  "depends": {
    "fabricloader": "*",
    "minecraft": "1.0.0-alpha.1.2.1"
  },

  "custom": {
    "modmenu:api": true
  }
}
