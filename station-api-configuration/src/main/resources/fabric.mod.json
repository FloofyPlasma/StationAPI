{
  "schemaVersion": 1,
  "id": "station-api-configuration",
  "version": "${version}",

  "name": "Station API Configuration",
  "description": "Configuration package for StationAPI.",
  "authors": [
    "Modification Station",
    "FloofyPlasma"
  ],
  "contact": {
    "homepage": "https://github.com/FloofyPlasma/StationAPI",
    "sources": "https://github.com/FloofyPlasma/StationAPI",
    "issues": "https://github.com/FloofyPlasma/StationAPI/issues"
  },

  "license": "MIT",
  "icon": "assets/station-api-configuration/icon.png",

  "environment": "*",
  "entrypoints": {
    "gcapi3": [
      "com.floofyplasma.stationapi.config.StationConfig"
    ],
    "gcapi3:factory_provider": [
      "com.floofyplasma.stationapi.config.FactoryProvider"
    ]
  },

  "depends": {
    "fabricloader": "*",
    "minecraft": "1.0.0-alpha.1.2.1"
  },

  "custom": {
    "modmenu:showIfContained": true
  }
}
