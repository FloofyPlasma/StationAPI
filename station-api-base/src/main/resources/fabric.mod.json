{
  "schemaVersion": 1,
  "id": "station-api-base",
  "version": "${version}",

  "name": "Station API Base",
  "description": "Contains the essentials for Station API modules, such as an event bus.",
  "authors": [
    "Modification Station",
    "FloofyPlasma"
  ],
  "contact": {
    "homepage": "https://github.com/FloofyPlasma/StationAPI",
    "sources": "https://github.com/FloofyPlasma/StationAPI",
    "issues": "https://github.com/FloofyPlasma/StationAPI/issues"
  },

  "license": "MIT",
  "icon": "assets/station-api-base/icon.png",

  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      "com.floofyplasma.stationapi.api.StationAPI"
    ],
    "spasm:transformer": [
      "com.floofyplasma.stationapi.impl.transform.UnsafeEventsIntegration"
    ],
    "stationapi:event_bus": [
      "com.floofyplasma.stationapi.impl.mod.DeprecatedInitEventsImpl"
    ]
  },

  "depends": {
    "fabricloader": "*",
    "minecraft": "1.0.0-alpha.1.2.1"
  },

  "custom": {
    "modmenu:api": true
  }
}
